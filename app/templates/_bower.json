{
  "name": "<%= _s.slugify(_s.humanize(appname)) %>",
  "version": "0.0.0",
  "dependencies": {
    "json3": "3.3.2",
    "forms-angular": "^0.9.0",
    "angular-ui-bootstrap-bower": <% if(filters.bs3) { %>"^1.3.2"<% } %><% if(filters.bs2) { %>"~0.8.0"<% } %>,<% if(filters.bootstrap) { %><% if (filters.sass) { %>
    "bootstrap-sass-official": "~3.3.5",<% } %>
    "bootstrap": "~3.3.5",<% } %>
    "angular-resource": "^1",
    "angular-cookies": "^1",<% if(filters.ngroute) { %>
    "angular-route": "^1",<% } %><% if(filters.uibootstrap) { %>
    "angular-bootstrap": "~0.14.3",<% } %>
    "font-awesome": ">=4.1.0",
    "lodash": "3.10.1"<% if(filters.socketio) { %>,
    "angular-socket-io": "0.7.0"<% } %><% if(filters.uirouter) { %>,
    "angular-ui-router": "0.2.15"<% } %><% if (filters.uiDate) { %>,
    "fng-ui-date": "*"<% } %><% if (filters.jqUpload) { %>,
    "fng-jq-upload": "*"<% } %><% if (filters.reports) { %>,
    "fng-reports": "*"<% } %><% if (filters.ckeditor) { %>,
    "ng-ckeditor": "0.2.1",
    "ckeditor": "*"<% } %><% if (filters.uiSelect) { %>,
    "fng-ui-select": "*"<% } %>
  },
  "devDependencies": {
    "angular-mocks": "^1.6.2"
  },<% if (filters.jqUpload) { %>
